<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App2.RegisterPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Enter UserName" Text="" x:Name="UserNameEntry"/>
            <Entry Placeholder="Enter Password" Text="" x:Name="PasswordEntry" />
            <Entry Placeholder="Enter First Name" Text="" x:Name="FirstEntry" />
            <Entry Placeholder="Enter Last Name" Text="" x:Name="LastEntry" />
            <Entry Placeholder="Enter Email Address" Text="" x:Name="EmailEntry" />
            <Entry Placeholder="Enter Age" Text="" x:Name="AgeEntry" />
            <Entry Placeholder="Tell us about yourself!" Text="" x:Name="Bio" />
            <Button Text="Register" Clicked="HandleClicked" />




        </StackLayout>
    </ContentPage.Content>
</ContentPage>